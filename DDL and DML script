-- Database: `railsys`
CREATE DATABASE IF NOT EXISTS `railsys`;
USE `railsys`;

-- Table: admin
CREATE TABLE IF NOT EXISTS `admin` (
  `adminId` INT(11) NOT NULL AUTO_INCREMENT,
  `permission` VARCHAR(255) NOT NULL,
  `email` VARCHAR(255) NOT NULL UNIQUE,
  `password` VARCHAR(255) NOT NULL,
  PRIMARY KEY (`adminId`)
);

-- Table: user
CREATE TABLE IF NOT EXISTS `user` (
  `userId` INT(255) NOT NULL AUTO_INCREMENT,
  `userName` VARCHAR(255) NOT NULL,
  `email` VARCHAR(255) NOT NULL,
  `password` VARCHAR(100) NOT NULL,
  `verification_code` VARCHAR(255) NOT NULL,
  `is_verified` VARCHAR(50) NOT NULL,
  PRIMARY KEY (`userId`)
);

INSERT INTO `user` (`userId`, `userName`, `email`, `password`, `verification_code`, `is_verified`) VALUES
(5, 'Goh', 'teckangoh@gmail.com', '$2y$10$TUFwYhXcV12hsN1Te84sfeI2.oSgpurtE30jVZayJCq7l3Regoswa', '3f55c7da196513a4e9dd051daf88e338', '1');

-- Table: notification
CREATE TABLE IF NOT EXISTS `notification` (
  `notificationId` INT(11) NOT NULL AUTO_INCREMENT,
  `message` TEXT NOT NULL,
  `timestamp` DATETIME NOT NULL,
  `recipient` VARCHAR(255) NOT NULL,
  PRIMARY KEY (`notificationId`)
);

-- Table: price
CREATE TABLE IF NOT EXISTS `price` (
    `priceId` INT(11) NOT NULL AUTO_INCREMENT,
    `amount` DECIMAL(10,2) NOT NULL,
    PRIMARY KEY (`priceId`)
);

INSERT INTO `price` (`priceId`, `amount`) VALUES
(1, 13.00),
(2, 15.00),
(3, 17.00);

-- Table: train
CREATE TABLE `train` (
  `trainNo` int(11) NOT NULL,
  `trainName` varchar(255) NOT NULL,
  `priceId` int(11) DEFAULT NULL,
  PRIMARY KEY (`trainNo`),
  KEY `fk_price` (`priceId`)
);

INSERT INTO `train` (`trainNo`, `trainName`, `priceId`) VALUES
