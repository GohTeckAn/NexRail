-- Adding Foreign Key Constraints
ALTER TABLE `seat`
  ADD CONSTRAINT `fk_trainNo` FOREIGN KEY (`trainNo`) REFERENCES `train` (`trainNo`),
  ADD CONSTRAINT `fk_user` FOREIGN KEY (`userId`) REFERENCES `user` (`userId`);

ALTER TABLE `train`
  ADD CONSTRAINT `fk_price` FOREIGN KEY (`priceId`) REFERENCES `price` (`priceId`);

ALTER TABLE `trainschedule`
  ADD CONSTRAINT `fk_train` FOREIGN KEY (`trainNo`) REFERENCES `train` (`trainNo`),
  ADD CONSTRAINT `trainschedule_ibfk_1` FOREIGN KEY (`priceId`) REFERENCES `price` (`priceId`) ON DELETE SET NULL;

-- Adjust AUTO_INCREMENT values
ALTER TABLE `price`
  MODIFY `priceId` int(11) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=4;

ALTER TABLE `seat`
  MODIFY `seatId` int(11) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=181;

ALTER TABLE `trainschedule`
  MODIFY `scheduleId` int(11) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=3046;

ALTER TABLE `user`
  MODIFY `userId` int(255) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=6;
