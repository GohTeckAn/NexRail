(9272, 'ETS', 2),
(9274, 'ETS', 2),
(9420, 'ETS', 2);

-- Table: seat
CREATE TABLE `seat` (
  `seatId` int(11) NOT NULL,
  `trainNo` int(11) DEFAULT NULL,
  `seatNumber` varchar(10) NOT NULL,
  `status` enum('available','occupied') NOT NULL,
  `userId` int(11) DEFAULT NULL,
  PRIMARY KEY (`seatId`),
  KEY `fk_trainNo` (`trainNo`),
  KEY `fk_user` (`userId`)
);

